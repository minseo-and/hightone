<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hightone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hightone">
        <activity
            android:name=".minseo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".minseo.GoodActivity"
            android:exported="false" />
        <activity
            android:name=".minseo.RecordsActivity"
            android:exported="false" />
        <activity
            android:name=".minseo.HopeActivity"
            android:exported="false" />
        <activity
            android:name=".minseo.SharePageActivity"
            android:exported="false" />
        <activity
            android:name=".minseo.FriendshipRecordsActivity"
            android:exported="true" />
    </application>

</manifest>